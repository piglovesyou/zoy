{namespace g.factory}


/**
 * @param context
 * @param id
 */
{template .create}
  {let $self: $context[$id] /}
  {switch $self.component}
    {case 'g.device.Button'}
      {call g.soy.device.Button.main data="all" /}
    {case 'app.Button'}
      {call g.soy.device.Button.main data="all" /}
    {default}
      <script>throw new Error('Add "{$self.component}" to factory.soy.');</script>
  {/switch}
{/template}
