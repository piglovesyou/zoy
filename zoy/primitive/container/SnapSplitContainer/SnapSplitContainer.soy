{namespace zoy.primitive.container.SnapSplitContainer.soy}

/***/
{template .main}
  <div {call zoy.common.soy.attribute data="all"}
         {param classNames: ['zoy-splitcontainer', 'zoy-snapsplitcontainer'] /}
       {/call}
  >
    {call zoy.primitive.container.SplitContainer.soy.containers data="all" /}
    {call .handle_ /}
  </div>
{/template}

/***/
{template .handle_ private="true"}
  <div class="goog-splitpane-handle">
    <div class="zoy-snapsplitcontainer__handle-btnwrap">
      <div class="zoy-snapsplitcontainer__handle-btn zoy-snapsplitcontainer__handle-btn-topleft">{sp}</div>
      <div class="zoy-snapsplitcontainer__handle-btn zoy-snapsplitcontainer__handle-btn-bottomright">{sp}</div>
    </div>
  </div>
{/template}

