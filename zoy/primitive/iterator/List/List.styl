.goog-list
  overflow: auto

.goog-list-item
  height: 30px
  line-height: 30px
  white-space: nowrap
  overflow: hidden
  text-overflow: ellipsis

.goog-list-item:nth-child(odd)
  background-color: #EBEFF9

.goog-list-item:hover
  color: $list-group-active-color
  background-color: $list-group-active-bg
